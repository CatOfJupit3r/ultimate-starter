{
  "name": "startername",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "docker compose -f ./docker-compose.dev.yml up -d && moon run :dev",
    "build": "moon run :build",
    "test": "moon run :test",
    "check-types": "moon run :check-types",
    "prettier": "moon run :prettier",
    "dev:web": "moon run web:dev",
    "dev:server": "moon run server:dev",
    "lint": "moon run :lint",
    "lint:fix": "moon run :lint -- --fix",
    "clean:modules": "tsx scripts/clean.ts",
    "prepare": "husky",
    "commit": "cz",
    "build:deps": "moon run shared:build",
    "update:workspaces": "pnpm update --interactive --recursive"
  },
  "devDependencies": {
    "@moonrepo/cli": "^1.31.3",
    "@types/node": "^24.0.0",
    "commitizen": "4.3.1",
    "cz-git": "1.12.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "rimraf": "6.1.2",
    "tsx": "^4.19.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./cz.config.cjs"
    }
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": "24.13.0",
    "pnpm": ">=10.0.0"
  }
}
