{
  "name": "startername",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "@eslint/compat": "2.0.0",
      "@eslint/js": "9.39.2",
      "@stylistic/eslint-plugin": "5.6.1",
      "eslint": "9.39.2",
      "eslint-config-airbnb-extended": "2.3.3",
      "eslint-config-prettier": "10.1.8",
      "eslint-import-resolver-typescript": "4.4.4",
      "eslint-plugin-import-x": "4.16.1",
      "eslint-plugin-n": "17.23.1",
      "eslint-plugin-prettier": "5.5.4",
      "prettier": "3.7.4",
      "typescript-eslint": "8.49.0",
      "@typescript-eslint/eslint-plugin": "8.49.0",
      "@typescript-eslint/parser": "8.49.0",
      "@types/bun": "1.3.4",
      "@trivago/prettier-plugin-sort-imports": "6.0.0",
      "zod": "4.1.13",
      "globals": "15.15.0",
      "typescript": "5.9.3",
      "dotenv": "17.2.3",
      "lodash-es": "4.17.21",
      "@types/lodash-es": "4.17.12",
      "tsdown": "0.17.4",
      "@orpc/contract": "1.12.3",
      "@orpc/server": "1.12.3",
      "@orpc/client": "1.12.3",
      "@orpc/tanstack-query": "1.12.3",
      "@orpc/zod": "1.12.3",
      "@orpc/openapi": "1.12.3",
      "better-auth": "1.4.5",
      "patch-package": "github:CatOfJupit3r/patch-package",
      "@typescript/native-preview": "7.0.0-dev.20251214.1"
    }
  },
  "scripts": {
    "dev": "docker compose -f ./docker-compose.dev.yml up -d &&turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "check-types": "turbo check-types",
    "prettier": "turbo prettier",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "clean:modules": "bun run scripts/clean.ts",
    "prepare": "husky",
    "commit": "cz",
    "build:deps": "turbo run build --filter=@startername/shared",
    "update:workspaces": "bun update --interactive --filter=\"*\""
  },
  "devDependencies": {
    "@types/bun": "1.3.4",
    "commitizen": "4.3.1",
    "cz-git": "1.12.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "rimraf": "6.1.2",
    "turbo": "2.6.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./cz.config.cjs"
    }
  },
  "packageManager": "bun@1.2.21"
}
