{
  "name": "startername",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "@eslint/compat": "^1.4.1",
      "@eslint/js": "^9.39.1",
      "@stylistic/eslint-plugin": "^3.1.0",
      "eslint": "^9.39.1",
      "eslint-config-airbnb-extended": "^2.3.3",
      "eslint-config-prettier": "^10.1.8",
      "eslint-import-resolver-typescript": "^4.4.4",
      "eslint-plugin-import-x": "^4.16.1",
      "eslint-plugin-n": "^17.23.1",
      "eslint-plugin-prettier": "^5.5.4",
      "prettier": "^3.6.2",
      "typescript-eslint": "^8.46.1",
      "@typescript-eslint/eslint-plugin": "^8.40.0",
      "@typescript-eslint/parser": "^8.40.0",
      "@types/bun": "^1.2.23",
      "@trivago/prettier-plugin-sort-imports": "^5.2.2",
      "zod": "^4.1.13",
      "globals": "^15.15.0",
      "typescript": "^5.9.3",
      "dotenv": "^17.2.3",
      "lodash-es": "^4.17.21",
      "@types/lodash-es": "^4.17.12",
      "tsdown": "^0.17.0",
      "@orpc/contract": "^1.12.2",
      "@orpc/server": "^1.11.3",
      "@orpc/client": "^1.11.3",
      "@orpc/tanstack-query": "^1.11.3",
      "@orpc/zod": "^1.11.3",
      "@orpc/openapi": "^1.11.3",
      "better-auth": "1.4.5",
      "patch-package": "github:CatOfJupit3r/patch-package",
      "@typescript/native-preview": "^7.0.0-dev.20251207.1"
    }
  },
  "scripts": {
    "dev": "docker compose -f ./docker-compose.dev.yml up -d &&turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "check-types": "turbo check-types",
    "prettier": "turbo prettier",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "clean:modules": "bun run scripts/clean.ts",
    "prepare": "husky",
    "commit": "cz",
    "build:deps": "turbo run build --filter=@startername/shared"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "turbo": "^2.6.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./cz.config.cjs"
    }
  },
  "packageManager": "bun@1.2.21"
}
