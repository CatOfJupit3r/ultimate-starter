{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "plugins": ["prettier-plugin-classnames", "prettier-plugin-tailwindcss", "@trivago/prettier-plugin-sort-imports"],
  "endOfLine": "lf",
  "importOrderSeparation": true,
  "importOrder": ["<THIRD_PARTY_MODULES>", "@startername/(.*)$", "^@~/(.*)$", "^[./]"]
}
