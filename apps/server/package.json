    "better-call": "^1.0.24",
    "dotenv": "catalog:",
    "hono": "^4.8.2",
    "hono-openapi": "^1.1.1",
    "lodash-es": "catalog:",
    "mongoose": "^8.19.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@eslint/compat": "catalog:",
    "@eslint/js": "catalog:",
    "@stylistic/eslint-plugin": "catalog:",
