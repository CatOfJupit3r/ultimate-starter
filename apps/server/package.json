    "@typegoose/typegoose": "13.0.0",
    "better-auth": "catalog:",
    "dotenv": "catalog:",
    "hono": "4.11.3",
    "hono-openapi": "1.1.2",
    "ioredis": "5.8.2",
    "lodash-es": "catalog:",
    "mongoose": "9.1.1",
    "reflect-metadata": "0.2.2",
    "safe-stable-stringify": "2.5.0",
    "tsyringe": "4.10.0",
    "winston": "3.19.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@eslint/compat": "catalog:",
    "@eslint/js": "catalog:",
    "@stylistic/eslint-plugin": "catalog:",
