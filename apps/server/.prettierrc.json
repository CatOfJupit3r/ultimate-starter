{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "endOfLine": "lf",
  "importOrderParserPlugins": ["typescript", "classProperties", "decorators-legacy"],
  "importOrderSeparation": true,
  "importOrder": ["<THIRD_PARTY_MODULES>", "@startername/(.*)$", "^@~/(.*)$", "^[./]"]
}
