{
  "name": "@startername/shared",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./globals": {
      "types": "./dist/globals.d.mts",
      "default": "./dist/globals.d.mts"
    },
    "./constants/*": {
      "types": "./dist/constants/*.d.mts",
      "default": "./dist/constants/*.mjs"
    },
    "./enums/*": {
      "types": "./dist/enums/*.d.mts",
      "default": "./dist/enums/*.mjs"
    },
    "./contract/*": {
      "types": "./dist/contract/*.d.mts",
      "default": "./dist/contract/*.mjs"
    },
    "./helpers/*": {
      "types": "./dist/helpers/*.d.mts",
      "default": "./dist/helpers/*.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "prepare": "pnpm run build",
    "dev": "tsdown --watch --no-clean",
    "prettier": "prettier --write .",
    "check-types": "tsgo --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "prettier": "@ultimate-starter/prettier-config",
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@typescript/native-preview": "catalog:",
    "@ultimate-starter/eslint-config": "workspace:*",
    "@ultimate-starter/prettier-config": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "typescript": "catalog:"
  },
  "dependencies": {
    "@orpc/contract": "catalog:",
    "dotenv": "catalog:",
    "lodash-es": "catalog:",
    "better-auth": "catalog:",
    "zod": "catalog:"
  }
}
