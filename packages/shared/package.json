{
  "name": "@startername/shared",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./constants/*": {
      "types": "./dist/constants/*.d.mts",
      "default": "./dist/constants/*.mjs"
    },
    "./enums/*": {
      "types": "./dist/enums/*.d.mts",
      "default": "./dist/enums/*.mjs"
    },
    "./contract/*": {
      "types": "./dist/contract/*.d.mts",
      "default": "./dist/contract/*.mjs"
    },
    "./helpers/*": {
      "types": "./dist/helpers/*.d.mts",
      "default": "./dist/helpers/*.mjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "prepare": "bun run build",
    "dev": "tsdown --watch"
  },
  "devDependencies": {
    "tsdown": "catalog:",
    "@trivago/prettier-plugin-sort-imports": "catalog:",
    "prettier": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@orpc/contract": "catalog:",
    "dotenv": "catalog:",
    "lodash": "catalog:",
    "better-auth": "catalog:",
    "zod": "catalog:"
  }
}
